% my $msg = stash 'message';
% $msg ||= 'Page not found';
% my $detail = stash 'detail';
% $detail ||= '';

% layout config 'layout';

<div class="page-header">
  <h2><%= l $msg %></h2>
  <%= l $detail %>
</div>

<p><%= l 'Please continue to the' %> <%= link_to site_home => begin %><%= l 'home page' %><% end %>.</p>
